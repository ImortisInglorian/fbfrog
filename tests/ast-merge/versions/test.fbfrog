dir "."

version *.minor(0)
	define MINOR 0
endversion

version major(1)
	define MAJOR 1
endversion

version major(2)
	define MAJOR 2
endversion

version major(3)
	define MAJOR 3
	version minor(1)
		define MINOR 1
	endversion
endversion

version major(4)
	define MAJOR 4
endversion
version major(4).minor(1)
	define MINOR 1
endversion

version major(5)
	define MAJOR 5
	version minor(1)
		define MINOR 1
	endversion
	version minor(2)
		define MINOR 2
	endversion
endversion

version major(6)
	define MAJOR 6
	version minor(1)
		define MINOR 1
	endversion
	version minor(2)
		define MINOR 2
	endversion
	version minor(3)
		define MINOR 3
	endversion
endversion

version *.minor(99)
	define MINOR 99
endversion
